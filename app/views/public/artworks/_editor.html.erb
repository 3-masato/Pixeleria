<pixeleria-editor
  id="pixeleria-editor"
  artwidth="<%= width %>"
  artheight="<%= height %>"
  dotsize="<%= dotsize %>"
></pixeleria-editor>
<div class="flex justify-evenly items-center mt-6">
  <%= component(
    "shared/button",
    style: :solid,
    color: :pink,
    class: "w-32 h-8 text-sm",
    id: "post-button"
  ) do %>
    <%= t("artwork.submit") %>
  <% end %>
</div>
